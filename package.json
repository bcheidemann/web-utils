{
  "name": "@bcheidemann/web-utils",
  "version": "0.0.0-development",
  "description": "Utilities for simplifying your javascript code in the web.",
  "repository": "https://github.com/bcheidemann/web-utils",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "test": "jest",
    "build": "gulp build",
    "coverage": "jest --coverage --coverageReporters=\"text\" --coverageReporters=\"text-summary\"",
    "serve-coverage": "live-server ./coverage/lcov-report",
    "docs": "typedoc --options typedoc.json && cp -a docs/.typedoc/. .",
    "rmdocs": "exitzero rm README.md && exitzero rm modules.md && exitzero rm -rf interfaces",
    "semantic-release": "semantic-release"
  },
  "author": "Ben Heidemann",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/gulp": "^4.0.8",
    "@types/jest": "^26.0.23",
    "exitzero": "^1.0.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "jest": "^26.6.3",
    "live-server": "^1.2.1",
    "semantic-release": "^17.4.3",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.7.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
